<!--Importar carpeta models del proyecto-->
@using MyPracticeCass.Models
@model List<Productos>

@{
    ViewBag.Title = "MostrarProductos";
}

<h2>MostrarProductos</h2>
@if (Model.Count() > 0)
{
    foreach (var i in Model)
    {
        <p>@i.IdProducto</p>
        <p>@i.DescripcionProducto</p>
        <p>@i.CantidadProducto</p>
        <p>@i.PrecioProducto</p>
    }
}
else
{
    <h2>¡No hay productos para mostrar!</h2>
}